<h3>アカウントサービス</h3>
<!-- <p id="user_name"><%# current_user.email %>さん</p> -->



<div id="box-parent" class="row col-md-6 col-md-offset-3">
  <%= link_to(settings_addresses_path, class: 'content col-md-4 col-md-offset-1') do %>
    <%= image_tag 'address_home.png' %>
    <strong>住所一覧</strong>
  <% end %>

  <%= link_to(settings_credits_path, class: 'content col-md-4 col-md-offset-2') do %>
    <%= image_tag 'credit.png' %>
    <strong>お支払い方法</strong>
  <% end %>

  <%= link_to(reviews_path, class: 'content col-md-4 col-md-offset-1') do %>
    <strong>購入した商品をレビューする</strong>
  <% end %>


<div class="text-center">
<%= link_to "退会する", user_registration_path(current_user.id), data: { confirm: "退会すると今までのデータを見ることができません？それでもいいですか？" }, method: :delete, id: 'delete_button', class: 'btn btn-warning' %>
</div>


